{
  "name": "patjacobs-com",
  "version": "4.0.0",
  "description": "Portfolio site for Pat Jacobs",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "debug": "yarn generate && NODE_OPTIONS='--inspect' next dev",
    "build": "yarn generate && next build",
    "start": "yarn dev",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "next lint",
    "lint:fix": "yarn lint --fix",
    "lint:tsc": "tsc --noEmit",
    "lint:commit": "yarn lint:tsc && lint-staged",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "tdd": "jest --watch",
    "generate": "graphql-codegen --config codegen.js",
    "prepare": "husky install",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@babel/core": "^7.17.0",
    "@next/env": "^12.0.10",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.22",
    "@types/redux-promise": "^0.5.28",
    "@types/styled-components": "^5.1.22",
    "@types/typography": "^0.16.4",
    "babel-plugin-styled-components": "^2.0.2",
    "date-fns": "^2.28.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "framer-motion": "^6.3.3",
    "graphcms-image": "^1.1.0-beta4",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "hex-to-rgba": "^2.0.1",
    "next": "^12.0.10",
    "next-redux-wrapper": "^7.0.5",
    "polished": "^4.1.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-plx": "^1.3.17",
    "react-query": "^3.34.14",
    "react-redux": "^8.0.1",
    "redux": "^4.1.2",
    "redux-promise": "^0.6.0",
    "styled-components": "^5.3.3",
    "styled-normalize": "^8.0.7",
    "ts-jest": "^28.0.1",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-syntax-typescript": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@graphql-codegen/cli": "^2.5.0",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/plugin-helpers": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.3",
    "@graphql-codegen/typescript-graphql-request": "^4.3.4",
    "@graphql-codegen/typescript-operations": "^2.2.4",
    "@graphql-codegen/typescript-react-query": "^3.5.1",
    "@storybook/addon-actions": "^6.5.0-alpha.42",
    "@storybook/addon-essentials": "^6.5.0-alpha.42",
    "@storybook/addon-interactions": "^6.5.0-alpha.42",
    "@storybook/addon-links": "^6.5.0-alpha.42",
    "@storybook/builder-webpack5": "^6.5.0-alpha.42",
    "@storybook/manager-webpack5": "^6.5.0-alpha.42",
    "@storybook/react": "^6.5.0-alpha.42",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/theming": "^6.5.0-alpha.42",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "^28.0.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-remove-graphql-queries": "^4.6.0",
    "babel-preset-react-app": "^10.0.1",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "^0.5.7",
    "husky": "^7.0.0",
    "jest": "^28.0.3",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^18.1.0",
    "redux-devtools-extension": "^2.13.9",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}