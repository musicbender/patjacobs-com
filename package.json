{
  "name": "patjacobs-com",
  "version": "5.0.0-beta1",
  "description": "Portfolio site for Pat Jacobs",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "debug": "yarn generate && NODE_OPTIONS='--inspect' next dev",
    "build": "yarn generate && next build",
    "start": "yarn dev",
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "next lint",
    "lint:fix": "yarn lint --fix",
    "lint:tsc": "tsc --noEmit",
    "lint:commit": "yarn lint:tsc && lint-staged",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "tdd": "jest --watch",
    "generate": "graphql-codegen --config codegen.js",
    "prepare": "husky install && panda codegen",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@next/env": "^14.0.4",
    "@reduxjs/toolkit": "^2.0.1",
    "@tanstack/react-query": "^5.15.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-redux": "^7.1.33",
    "@types/redux-promise": "^0.5.32",
    "@types/typography": "^0.16.7",
    "date-fns": "^3.0.6",
    "framer-motion": "^10.16.16",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "hex-to-rgba": "^2.0.1",
    "jsondiffpatch": "^0.6.0",
    "next": "^14.0.4",
    "next-pwa": "^5.6.0",
    "next-redux-wrapper": "^8.1.0",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plx": "^2.1.2",
    "react-redux": "^9.0.4",
    "redux": "^5.0.1",
    "redux-promise": "^0.6.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/fragment-matcher": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/plugin-helpers": "^5.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^6.1.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-query": "^6.0.0",
    "@pandacss/dev": "^0.23.0",
    "@storybook/addon-actions": "^7.6.6",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/builder-webpack5": "^7.6.6",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^7.6.6",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.6.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-remove-graphql-queries": "^5.13.0",
    "babel-preset-react-app": "^10.0.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "15.2.0",
    "prettier": "^3.1.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^18.2.0",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
