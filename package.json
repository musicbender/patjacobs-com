{
  "name": "patjacobs-com",
  "version": "4.0.0",
  "description": "Portfolio site for Pat Jacobs",
  "private": true,
  "scripts": {
    "dev": "yarn generate && sh scripts/start-dev.sh",
    "build": "yarn generate && next build",
    "start": "yarn dev",
    "build:local": "sh scripts/build-prod-local.sh",
    "build:serve": "yarn build:local && gatsby serve",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"types/**/*.{ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "lint:tsc": "tsc --noEmit",
    "lint:commit": "yarn lint:tsc && lint-staged",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "jest",
    "tdd": "jest --watch",
    "generate": "graphql-codegen --config codegen.js"
  },
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@next/env": "^12.0.7",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-redux": "^7.1.5",
    "@types/redux-promise": "^0.5.28",
    "@types/styled-components": "^5.1.0",
    "@types/typography": "^0.16.2",
    "babel-plugin-styled-components": "^1.10.0",
    "date-fns": "^2.14.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "graphcms-image": "^1.1.0-beta4",
    "graphql": "^15.7.2",
    "graphql-request": "^3.7.0",
    "hex-to-rgba": "^2.0.1",
    "immutability-helper": "^3.0.1",
    "polished": "^3.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-plx": "^1.3.15",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-promise": "^0.6.0",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7",
    "ts-jest": "^26.1.0",
    "typescript": "4.5.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-typescript": "^7.10.1",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/plugin-helpers": "^1.18.4",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-graphql-request": "^3.1.0",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-essentials": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/react": "^5.2.6",
    "@storybook/theming": "^5.2.6",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-remove-graphql-queries": "^2.8.2",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "jest": "^26.0.1",
    "lint-staged": "^10.1.2",
    "prettier": "^1.16.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^16.12.0",
    "redux-devtools-extension": "^2.13.8",
    "ts-loader": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:commit"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
