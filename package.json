{
  "name": "patjacobs-com",
  "version": "1.0.0",
  "description": "Portfolio site for Pat Jacobs: Web Engineer",
  "private": true,
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build",
    "build:local": "sh scripts/build-prod-local.sh",
    "build:serve": "yarn build:local && gatsby serve",
    "dev": "sh scripts/start-dev.sh",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"types/**/*.{ts,tsx}\"",
    "lint:fix": "yarn lint -- --fix",
    "start": "yarn dev",
    "test": "jest",
    "tdd": "jest --watch"
  },
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@storybook/theming": "^5.2.6",
    "@types/react": "16.8.1",
    "@types/react-dom": "16.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/react-redux": "^7.1.5",
    "@types/redux-promise": "^0.5.28",
    "@types/styled-components": "^4.1.12",
    "@types/typography": "^0.16.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.2.6",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-netlify": "^2.1.25",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-rollbar": "^1.1.0",
    "gatsby-plugin-sharp": "^2.0.30",
    "gatsby-plugin-sitemap": "^2.0.10",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-plugin-typography": "^2.2.10",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^2.0.27",
    "gatsby-source-graphql": "^2.3.0",
    "gatsby-transformer-sharp": "^2.1.17",
    "graphcms-image": "^1.1.0-beta4",
    "hex-to-rgba": "^2.0.1",
    "immutability-helper": "^3.0.1",
    "polished": "^3.4.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-plx": "^1.3.15",
    "react-redux": "6.0.1",
    "react-typography": "^0.16.19",
    "redux": "^4.0.4",
    "redux-promise": "^0.6.0",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.7",
    "ts-jest": "^24.2.0",
    "typescript": "^3.3.3333",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.22",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "gatsby-plugin-graphql-codegen": "^2.0.1",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "lint-staged": "^10.1.2",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.12.0",
    "redux-devtools-extension": "^2.13.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
